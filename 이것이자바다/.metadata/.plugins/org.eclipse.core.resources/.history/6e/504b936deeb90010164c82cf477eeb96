package ch14.sec04;

public class ThreadNameEx {

	public static void main(String[] args) {
		Thread mainT = Thread.currentThread();
		System.out.println(mainT.getName());
		
		Thread t1 = new Thread() {
			public void run() {
				System.out.println(getName()); 	
			}
		};
		t1.start();
		
		Thread t2 = new Thread() {
			t2.setName("t2내가지은");
			public void run() {
				System.out.println(getName());
			}
		};
		t2.start();

	}

}
